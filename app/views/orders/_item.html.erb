<tr>
  <td>
    <%= image_tag item.image.tiny %>
  </td>
  <td>
    <h4><%= item.name %></h4>
    <br>
    <p><%= item.description %></p>
  </td>
  <td>
    <%= humanized_money_with_symbol item.price %>
  </td>
  <td>
    <%= quantity %>
  </td>
  <td>
    <%= humanized_money_with_symbol item.price * quantity %>
  </td>
</tr>
